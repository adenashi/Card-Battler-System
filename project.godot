; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Card Battler System"
run/main_scene="res://Scenes/Gameplay/battlefield.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/image="res://Assets/Images/Adenashi Games Splash Screen Black.png"
config/icon="res://Assets/Icons/GameIcon.png"

[autoload]

Dispatch="*res://Singletons/Managers/Dispatch.gd"
GameManager="*res://Singletons/Managers/GameManager.gd"
EffectsDb="*res://Singletons/Databases/EffectsDB.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/initial_position_type=0
window/size/initial_position=Vector2i(340, 30)
window/size/window_width_override=1600
window/size/window_height_override=900
window/stretch/mode="viewport"
